<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<HBox fx:id="root" prefHeight="400.0" prefWidth="520.0" spacing="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="reversiapp.ReversiGameController">
   <children>
   
   	<VBox prefHeight="400.0" prefWidth="156.0" spacing="10">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <children>
            <Label fx:id="currPlayer">
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="firstPlayerPoints" />
            <Label fx:id="secondPlayerPoints" />
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#goHome" prefHeight="30.0" prefWidth="100.0" text="Home">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
         </children>
   	</VBox>
   </children>
</HBox>
